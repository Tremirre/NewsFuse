{
    "manifest_version": 3,
    "name": "NewsFuse",
    "version": "0.1.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": {
        "128": "icons/128.png",
        "64": "icons/64.png",
        "32": "icons/32.png",
        "16": "icons/16.png"
    }
}